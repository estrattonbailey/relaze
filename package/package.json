{
  "name": "relaze",
  "version": "1.0.0",
  "description": "A modern image lazy loading library for React.",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/estrattonbailey/relaze",
  "author": "estrattonbailey",
  "scripts": {
    "lint": "standard 'index.js'",
    "prebuild": "npm run lint",
    "build": "NODE_ENV=production babel index.js -d dist/",
    "watch": "NODE_ENV=development babel -w --no-comments index.js -d dist/",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "lazy load"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "standard": "^8.6.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "srraf": "^0.1.0"
  }
}
